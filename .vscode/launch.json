{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200/main-page",
            "pathMapping": {
                "/": "${workspaceFolder}"
            },
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*",
                "webpack:///src/*": "${webRoot}/*",
                "webpack:///*": "*",
                "webpack:///./~/*": "${webRoot}/node_modules/*",
                "meteor://ðŸ’»app/*": "${webRoot}/*",
                "webpack:/*": "${webRoot}/*"
            },
            "smartStep": true
        }
    ]
}